<h1> sign up </h1>
<%= form_with model: @user do |form| %>
    <div>
        <%= form.label :first_name %><br>
        <%= form.text_field :first_name%>
    </div>
    <div>
        <%= form.label :last_name %><br>
        <%= form.text_field :last_name %>
    </div>
    <div>
        <%= form.label :email %><br>
        <%= form.email_field :email %>
    </div>
    <div>
        <%= form.label :password %><br>
        <%= form.password_field :password %>
    </div>
    <div>
        <br>
        <%= form.submit %>
    </div>
<% end %>    